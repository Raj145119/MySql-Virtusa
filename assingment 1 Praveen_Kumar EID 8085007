Q5. 
	select sname from salespeople 
		where city='London' OR city='Sheohar';
		
Q6. 
	cl scr;

Q7.  
	SQL> select sysdate from dual;

SYSDATE
---------
05-DEC-20

Q8.
	
SQL> desc salespeople;
 Name                                      Null?    Type
 ----------------------------------------- -------- ----------------------------
 SNUM                                      NOT NULL NUMBER(5)
 SNAME                                              CHAR(15)
 CITY                                               VARCHAR2(39)
 COMM                                               NUMBER(11,2)

9Q.
SQL> SELECT COUNT(SNUM) FROM salespeople; 
	or
		SELECT COUNT(*) FROM salespeople;

COUNT(SNUM)
-----------
         20
		 
10Q.

SQL> insert into salespeople 
	values (21,'aditya','London',234.90);

1 row created.

11Q.
SQL> select snum from salespeople 
		where comm is null;

no rows selected

12Q.
SQL> select count(sname), sum(comm) 
		from salespeople 
			where comm is not null;

COUNT(SNAME)  SUM(COMM)
------------ ----------
          21    6978.92

